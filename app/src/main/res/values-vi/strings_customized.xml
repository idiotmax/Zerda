<resources>
    <!--Dialog-->
    <!--dialog title for introducing Firefox Lite Travel Discovery-->
    <string name="travel_dialog_1_title">Khám phá thêm!</string>
    <!--dialog description for introducing the feature. The %1$s here is Firefox Lite.-->
    <string name="travel_dialog_1_description">Thám hiểm những chân trời mới, khách sạn, nhà nghỉ và nhiều hơn trong Khám phá Du lịch của %1$s</string>
